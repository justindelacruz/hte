<!DOCTYPE html>
<html lang="en">
<head>
    <title>{% block title %}{% endblock %}HTE Enterprise - Steel and Aluminum Alloy Product Manufacturing</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="canonical" href="http://www.hteht.net{{ path(app.request.get('_route')) }}">
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />

    <meta property="og:title" content="">
    <meta property="og:type" content="article">
    <meta property="og:description" content="">
    <meta property="og:image" content="">
    <meta property="og:site_name" content="HTE Enterprise">
    <meta property="og:url" content="http://hte.nfshost.com{{ path(app.request.get('_route')) }}">
    {# <meta property="fb:admins" content="1360179707"> #}

    {% block stylesheets %}
        {% stylesheets 'bundles/htesite/css/first.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% stylesheets 'bundles/htesite/css/bootstrap.min.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% stylesheets 'bundles/htesite/css/lightbox.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% stylesheets 'bundles/htesite/css/main.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endblock %}

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

</head>
<body>
<div id="wrap">
    <a href="#main" class="sr-only">Skip to main content</a>

    <div id="top" class="h-header">
        <a href="{{ path('hte_index') }}">
            <span>
                <h1>HTE Enterprises</h1>
                <p>Steel and Aluminum Alloy Product Manufacturing</p>
            </span>
            <img src="{{ asset('bundles/htesite/images/header_logo.png') }}" class="img-responsive" />
        </a>
    </div>

    <nav class="h-nav-justified" role="navigation">
        <div class="container">
            <ul class="nav nav-justified">
                <li{% if app.request.get('_route') == 'hte_index' %} class="active"{% endif %}><a href="{{ path('hte_index') }}">Home</a></li>
                <li{% if app.request.get('_route') == 'hte_about' %} class="active"{% endif %}><a href="{{ path('hte_about') }}">About Us</a></li>
                <li{% if app.request.get('_route') == 'hte_product_samples' %} class="active"{% endif %}><a href="{{ path('hte_product_samples') }}">Product Samples</a></li>
                <li{% if app.request.get('_route') == 'hte_capabilities' %} class="active"{% endif %}><a href="{{ path('hte_capabilities') }}">Capabilities</a></li>
                <li{% if app.request.get('_route') == 'hte_contact' %} class="active"{% endif %}><a href="{{ path('hte_contact') }}">Contact</a></li>
            </ul>
        </div>
    </nav>

    <nav class="h-navbar navbar navbar-default" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>

            <a class="navbar-brand" href="#" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">Menu</a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav">
                <li{% if app.request.get('_route') == 'hte_index' %} class="active"{% endif %}><a href="{{ path('hte_index') }}">Home</a></li>
                <li{% if app.request.get('_route') == 'hte_about' %} class="active"{% endif %}><a href="{{ path('hte_about') }}">About Us</a></li>
                <li{% if app.request.get('_route') == 'hte_product_samples' %} class="active"{% endif %}><a href="{{ path('hte_product_samples') }}">Product Samples</a></li>
                <li{% if app.request.get('_route') == 'hte_capabilities' %} class="active"{% endif %}><a href="{{ path('hte_capabilities') }}">Capabilities</a></li>
                <li{% if app.request.get('_route') == 'hte_contact' %} class="active"{% endif %}><a href="{{ path('hte_contact') }}">Contact</a></li>
            </ul>

        </div>
    </nav>

    <div id="main" class="container {% block pageClass %}{% endblock %}" role="main">
        {% block fullWidthContainer %}{% endblock %}
        <div class="fixed-body">
            {% block container %}{% endblock %}
        </div>
    </div>

    <div class="h-footer" id="footer">
        <div class="container">
            <div class="row">
                <address class="col-sm-3">
                    <strong>{{ 'footer.section_1.title'|trans|raw }}</strong><br>
                    {{ 'footer.section_1.address_1'|trans|raw }}<br>
                    {{ 'footer.section_1.address_2'|trans|raw }}<br>
                    {{ 'footer.section_1.address_3'|trans|raw }}<br>
                    Tel: {{ 'footer.section_1.tel'|trans|raw }}<br>
                    Fax: {{ 'footer.section_1.fax'|trans|raw }}
                </address>
                <address class="col-sm-3">
                    <strong>{{ 'footer.section_2.title'|trans|raw }}</strong><br>
                    {{ 'footer.section_2.address_1'|trans|raw }}<br>
                    {{ 'footer.section_2.address_2'|trans|raw }}<br>
                    {{ 'footer.section_2.address_3'|trans|raw }}<br>
                    Tel: {{ 'footer.section_2.tel'|trans|raw }}<br>
                    Fax: {{ 'footer.section_2.fax'|trans|raw }}
                </address>
                <address class="col-sm-3">
                    <strong>{{ 'footer.section_3.title'|trans|raw }}</strong><br>
                    {{ 'footer.section_3.address_1'|trans|raw }}<br>
                    {{ 'footer.section_3.address_2'|trans|raw }}<br>
                    {{ 'footer.section_3.address_3'|trans|raw }}<br>
                    Tel: {{ 'footer.section_3.tel'|trans|raw }}<br>
                    Fax: {{ 'footer.section_3.fax'|trans|raw }}
                </address>

                <div class="col-sm-3">
                    <a href="{{ path('hte_contact') }}" class="h-button"><span class="glyphicon glyphicon-envelope"></span> Contact Us</a>

                    <div class="social">
                        <a href="http://facebook.com/hteht" class="icons-facebook"><span class="abs">Facebook</span></a>
                        <a href="http://linkedin.com/company/hte-enterprise" class="icons-linkedin"><span class="abs">LinkedIn</span></a>
                        <a href="http://gplus.to/hteht" class="icons-google_plus"><span class="abs">Google Plus</span></a>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-9 copyright">
                    {{ 'footer.copyright'|trans|raw }}
                </div>
            </div>
        </div>
    </div>
</div>

{% block javascripts %}
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
<script src="{{ asset('bundles/htesite/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('bundles/htesite/js/lightbox-2.6.min.js') }}"></script>
{% endblock %}
</body>
</html>
