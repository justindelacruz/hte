<!DOCTYPE html>
<html lang="en">
<head>
    <title>HTE Enterprises - Steel & Aluminum Alloy Product Manufacturing</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="canonical" href="http://hte.nfshost.com{{ path(app.request.get('_route')) }}">

    <meta property="og:title" content="">
    <meta property="og:type" content="article">
    <meta property="og:description" content="">
    <meta property="og:image" content="">
    <meta property="og:site_name" content="HTE Enterprise">
    <meta property="og:url" content="http://hte.nfshost.com{{ path(app.request.get('_route')) }}">
    {# <meta property="fb:admins" content="1360179707"> #}
    {# include font: http://www.google.com/fonts#UsePlace:use/Collection:Lato:300,400,700,300italic,400italic,700italic #}

    <!-- Bootstrap -->
    {% block stylesheets %}
        <link href="{{ asset('bundles/htesite/css/bootstrap.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/htesite/css/main.desktop.css') }}" rel="stylesheet">
    {% endblock %}

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

</head>
<body>
<div id="h-skip"><a href="#main">Skip to main content</a></div>

<div class="h-header">
    <a href="{{ path('hte_index') }}">
        <span>
            <h1>HTE Enterprises</h1>
            <h2>Steel & Aluminum Alloy Product Manufacturing</h2>
            </span>
        <img src="{{ asset('bundles/htesite/images/header_logo.png') }}" />
    </a>
</div>

<nav class="navbar navbar-default" role="navigation">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
            <li{% if app.request.get('_route') == 'hte_about' %} class="active"{% endif %}><a href="{{ path('hte_about') }}">About Us</a></li>
            <li{% if app.request.get('_route') == 'hte_product_samples' %} class="active"{% endif %}><a href="{{ path('hte_product_samples') }}">Product Samples</a></li>
            <li{% if app.request.get('_route') == 'hte_capabilities' %} class="active"{% endif %}><a href="{{ path('hte_capabilities') }}">Capabilities</a></li>
            <li{% if app.request.get('_route') == 'hte_contact' %} class="active"{% endif %}><a href="{{ path('hte_contact') }}">Contact</a></li>
        </ul>

    </div>
</nav>

<div id="main" class="container" role="main">
    {% block container %}{% endblock %}
</div>

<div class="h-footer" id="footer">
    <div class="container">
        <address class="col-md-3">
            <strong>{{ 'footer.section_1.title'|trans|raw }}</strong><br>
            {{ 'footer.section_1.address_1'|trans|raw }}<br>
            {{ 'footer.section_1.address_2'|trans|raw }}<br>
            {{ 'footer.section_1.address_3'|trans|raw }}<br>
            <abbr title="Telephone">Tel</abbr>: {{ 'footer.section_1.tel'|trans|raw }}<br>
            Fax: {{ 'footer.section_1.fax'|trans|raw }}
        </address>
        <div class="col-md-3">
            <strong>{{ 'footer.section_2.title'|trans|raw }}</strong>
            <p>
                <span>{{ 'footer.section_2.address_1'|trans|raw }}</span>
                <span>{{ 'footer.section_2.address_2'|trans|raw }}</span>
                <span>{{ 'footer.section_2.address_3'|trans|raw }}</span>
                <span><abbr title="Telephone">Tel</abbr>: {{ 'footer.section_2.tel'|trans|raw }}</span>
                <span>Fax: {{ 'footer.section_2.fax'|trans|raw }}</span>
            </p>
        </div>
        <div class="col-md-3">
            <strong>{{ 'footer.section_3.title'|trans|raw }}</strong>
            <p>
                <span>{{ 'footer.section_3.address_1'|trans|raw }}</span>
                <span>{{ 'footer.section_3.address_2'|trans|raw }}</span>
                <span>{{ 'footer.section_3.address_3'|trans|raw }}</span>
                <span>Tel: {{ 'footer.section_3.tel'|trans|raw }}</span>
                <span>Fax: {{ 'footer.section_3.fax'|trans|raw }}</span>
            </p>
        </div>

        <div class="col-md-3">
            <button>Contact Us</button>
            <span><a href="">Facebook</a></span>
            <span><a href="">LinkedIn</a></span>
            <span><a href="">Google+</a></span>
        </div>
    </div>

    <div class="copyright container">
        {{ 'footer.copyright'|trans|raw }}
    </div>
</div>

{% block javascripts %}
<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="https://code.jquery.com/jquery.js"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="{{ asset('bundles/htesite/js/bootstrap.min.js') }}"></script>
{% endblock %}
</body>
</html>
